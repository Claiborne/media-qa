include OpenPage

contents = []
file = File.new("/Users/wclaiborne/Desktop/new_missing.txt", "r")
while (line = file.gets)
  contents << line
end
file.close

puts "CONTENTS LENGTH #{contents.length}"

ids = []

contents.each do |c|
  if c.match(/^\d/)
    ids << c
  end
end

puts "IDS LENGTH #{ids.length}"
puts ids.uniq.length.to_s

ids.uniq!.each do |i|
  puts i
end
